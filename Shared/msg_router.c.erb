#include "msg_router.h"
#include <stddef.h>
#include <string.h>

typedef struct {
    const char   *api_name;
    msg_func_t    func;
} msg_entry_t;

% for entry in entries
void <%= entry.func_name %>(json_t *data);
% end

msg_entry_t entries[] = {
% for entry in entries
    { "<%= entry.msg_name %>", &<%= entry.func_name %> },
% end
    { NULL, NULL }
};

msg_func_t find_msg_handler(const char *api_name) {
    for (msg_entry_t *entry = entries; entry->api_name; entry++) {
        if (0 == strcmp(api_name, entry->api_name))
            return entry->func;
    }
    return NULL;
}
