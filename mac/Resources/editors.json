{
  "editors": [
    {
      "id": "com.sublimetext.2",
      "name": "Sublime Text 2",
      "cocoa-bundle-id": "com.sublimetext.2",
      "default-priority": 2
    },
    {
      "id": "com.macromates.textmate",
      "name": "TextMate",
      "cocoa-bundle-id": "com.macromates.textmate",
      "open-url-1": "txmt://open/?url=((fileURL))",
      "open-url-2": "txmt://open/?url=((fileURL))&line=((line))",
      "open-url-3": "txmt://open/?url=((fileURL))&line=((line))&column=((column))",
      "default-priority": 1
    }
  ]
}
