<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>LRCompilers</key>
	<array>
		<dict>
			<key>CommandLine</key>
			<array>
				<string>$(node)</string>
				<string>$(plugin)/node_modules/stylus/bin/stylus</string>
				<string>$(additional)</string>
				<string>-o</string>
				<string>$(dst_dir)</string>
				<string>$(src_path)</string>
			</array>
			<key>DestinationExtension</key>
			<string>css</string>
			<key>Errors</key>
			<array>
				<string>Error: ((file)):((line))\n(?:\s[^\n]*\n)*\s*\n((message))\n</string>
				<string>\w+Error: ((message))\n</string>
			</array>
			<key>Extensions</key>
			<array>
				<string>styl</string>
			</array>
			<key>Name</key>
			<string>Stylus</string>
			<key>ExpectedOutputDirectories</key>
			<array>
				<string>css</string>
				<string>styles</string>
				<string>stylesheet</string>
				<string>stylesheets</string>
			</array>
			<key>ImportRegExps</key>
			<array>
				<string>@import &quot;([^&quot;]+)&quot;</string>
				<string>@import &apos;([^&apos;]+)&apos;</string>
				<string>@import ([\w./]+)</string>
			</array>
			<key>DefaultImportedExts</key>
			<array>
				<string>styl</string>
			</array>
			<key>NonImportedExts</key>
			<array>
				<string>css</string>
			</array>
			<key>Options</key>
			<array>
				<dict>
					<key>Id</key>
					<string>debug-info</string>
					<key>Type</key>
					<string>checkbox</string>
					<key>Title</key>
					<string>Emit debug info for FireStylus (--firebug)</string>
					<key>OnArgument</key>
					<string>--firebug</string>
				</dict>
				<dict>
					<key>Id</key>
					<string>line-numbers</string>
					<key>Type</key>
					<string>checkbox</string>
					<key>Title</key>
					<string>Emit line numbers debug info (--line-numbers)</string>
					<key>OnArgument</key>
					<string>--line-numbers</string>
				</dict>
			</array>
		</dict>
	</array>
</dict>
</plist>
